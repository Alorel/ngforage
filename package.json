{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ngforage",
  "description": "localForage bindings for Angular",
  "version": "3.0.5",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "start": "ng serve",
    "test": "rimraf ./coverage && karma start"
  },
  "keywords": [
    "localforage",
    "binding",
    "angular",
    "indexeddb",
    "indexed",
    "db",
    "websql",
    "web",
    "sql",
    "local",
    "session",
    "storage",
    "localstorage",
    "sessionstorage",
    "pwa",
    "offline",
    "progressive",
    "web",
    "app"
  ],
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "licensePath": "LICENSE",
      "deleteDestPath": true,
      "umdModuleIds": {
        "localforage": "localforage",
        "typescript-lazy-get-decorator": "LazyGet"
      }
    },
    "whitelistedNonPeerDependencies": [
      "typescript-lazy-get-decorator"
    ]
  },
  "dependencies": {
    "typescript-lazy-get-decorator": "^1.0.0"
  },
  "peerDependencies": {
    "localforage": "^1.5.0",
    "@angular/common": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/core": "^6.0.0-rc.1 || ^6.0.0"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.0-rc.4",
    "@angular/common": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/compiler": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/compiler-cli": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/core": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/forms": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/platform-browser": "^6.0.0-rc.1 || ^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.1 || ^6.0.0",
    "@compodoc/gulp-compodoc": "^0.0.9",
    "@types/jasmine": "^2.6",
    "@types/karma": "^1.7",
    "@types/lodash": "^4.14.80",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.3.0",
    "@types/uuid": "^3.4.3",
    "angular2-template-loader": "^0.6.2",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.2.1",
    "core-js": "^2.5.4",
    "coveralls": "^3.0",
    "cross-spawn": "^6.0.5",
    "custom-tslint-formatters": "^2.1.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sequence": "^1.0.0",
    "html-minifier": "^3.5.13",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.8",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2",
    "karma-coverage-istanbul-reporter": "^1.3",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-mocha-reporter": "^2.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3",
    "karma-webpack": "^3.0.0",
    "localforage": "^1.5.0",
    "localstorage-browser-polyfill": "^0.1.0",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.8",
    "ng-packagr": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6",
    "rxjs": "6.0.0-rc.0 || ^6.0.0",
    "rxjs-compat": "^6.0.0-rc.0 || ^6.0.0",
    "ts-loader": "^3.0.0",
    "ts-node": "^6.0.0",
    "tsickle": "^0.27.2",
    "tslib": "^1.9.0",
    "tslint": "^5.8.0",
    "typescript": ">=2.7.2 <2.8.0",
    "uglify-es": "^3.3.9",
    "uuid": "^3.1.0",
    "webpack": "^3.0.0",
    "yamljs": "^0.3.0",
    "zone.js": ">=0.8.4 <1.0"
  },
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://alorel.github.io",
    "email": "a.molcanovas@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/ngforage.git"
  },
  "homepage": "https://github.com/Alorel/ngforage",
  "bugs": {
    "url": "https://github.com/Alorel/ngforage/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "patrykp57",
      "url": "https://github.com/patrykp57"
    }
  ],
  "greenkeeper": {
    "ignore": [
      "webpack",
      "ts-loader",
      "jasmine-core",
      "typescript"
    ]
  }
}
