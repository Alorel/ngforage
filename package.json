    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lazy-get-decorator": "^2.2.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20",
    "ng-packagr": "^11.2.0",
    "proto-decorator": "^3.0.3",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rxjs": "^6.6.3",
    "semantic-release": "^17.0.8",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
