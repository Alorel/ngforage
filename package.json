{
  "name": "ngforage",
  "description": "localForage bindings for Angular",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sync": "node build/sync-pkg.js",
    "prestart": "npm run sync",
    "prebuild:demo": "npm run sync",
    "prebuild:lib": "npm run sync",
    "pretest": "npm run sync && rimraf coverage",
    "prelint": "npm run sync",
    "start": "ng serve demo",
    "start:aot": "npm run start -- --aot",
    "build:demo": "ng build demo",
    "build:demo:prod": "npm run build:demo -- --prod --aot",
    "build:lib": "ng build ngforage",
    "watch:lib": "npm run build:lib -- --watch",
    "test": "ng test ngforage",
    "lint": "tslint -s ./node_modules/custom-tslint-formatters/formatters -t grouped -p ./projects/tsconfig.lint.json",
    "lint:fix": "npm run lint -- --fix",
    "precompodoc": "rimraf dist/demo/docs",
    "compodoc": "compodoc -c ./.compodocrc.yml"
  },
  "keywords": [
    "localforage",
    "binding",
    "angular",
    "indexeddb",
    "indexed",
    "db",
    "websql",
    "web",
    "sql",
    "local",
    "session",
    "storage",
    "localstorage",
    "sessionstorage",
    "pwa",
    "offline",
    "progressive",
    "web",
    "app"
  ],
  "peerDependencies": {
    "@angular/common": "^7.0.0-beta.0",
    "@angular/core": "^7.0.0-beta.0",
    "localforage": "^1.5.0",
    "typescript-lazy-get-decorator": "^1.0.0",
    "typescript-proto-decorator": "^2.0.0"
  },
