{
  "name": "ngforage",
  "description": "localForage bindings for Angular",
  "version": "10.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "sync": "node build/sync-pkg.js",
    "prestart": "npm --scripts-prepend-node-path=auto run sync",
    "prebuild:demo": "npm --scripts-prepend-node-path=auto run sync && rimraf dist/demo",
    "prebuild:lib": "npm --scripts-prepend-node-path=auto run sync && rimraf dist/ngforage",
    "pretest": "npm --scripts-prepend-node-path=auto run sync && rimraf coverage",
    "start": "ng serve demo",
    "build:demo": "ng build demo",
    "build:demo:prod": "npm --scripts-prepend-node-path=auto run build:demo -- --configuration production",
    "build:lib": "ng build ngforage",
    "watch:lib": "npm run build:lib -- --watch",
    "test": "node --no-deprecation ./node_modules/.bin/ng test ngforage --code-coverage",
    "precompodoc": "rimraf dist/demo/docs",
    "release": "semantic-release",
    "compodoc": "compodoc -c ./.compodocrc.yml"
  },
  "keywords": [
    "localforage",
    "binding",
    "angular",
    "indexeddb",
    "indexed",
    "db",
    "websql",
    "web",
    "sql",
    "local",
    "session",
    "storage",
    "localstorage",
    "sessionstorage",
    "pwa",
    "offline",
    "progressive",
    "web",
    "app"
  ],
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0",
    "localforage": "^1.10.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "2.1.3",
    "@angular-devkit/build-angular": "^16.1.5",
    "@angular/cli": "^16.1.5",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/compiler-cli": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@compodoc/compodoc": "^1.1.21",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.4",
    "@types/jasmine": "~4.3.5",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.2",
    "@types/uuid": "^9.0.2",
    "bluebird": "^3.7.2",
    "bootstrap-material-design": "^4.1.3",
    "doctoc": "^2.2.1",
    "glob": "^10.3.3",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lazy-get-decorator": "^2.2.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "ng-packagr": "^16.1.0",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "semantic-release": "^21.0.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "~5.1.6",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.1"
  },
  "author": {
    "name": "Arturas Molcanovas",
