{
  "name": "ngforage",
  "description": "localForage bindings for Angular",
  "version": "8.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "sync": "node build/sync-pkg.js",
    "prestart": "npm --scripts-prepend-node-path=auto run sync",
    "prebuild:demo": "npm --scripts-prepend-node-path=auto run sync && rimraf dist/demo",
    "prebuild:lib": "npm --scripts-prepend-node-path=auto run sync && rimraf dist/ngforage",
    "pretest": "npm --scripts-prepend-node-path=auto run sync && rimraf coverage",
    "prelint": "npm --scripts-prepend-node-path=auto run sync",
    "start": "ng serve demo",
    "start:aot": "npm --scripts-prepend-node-path=auto run start -- --aot",
    "build:demo": "ng build demo",
    "build:demo:prod": "npm --scripts-prepend-node-path=auto run build:demo -- --prod --aot",
    "build:lib": "ng build ngforage --prod",
    "postbuild:lib": "cd projects/ngforage && npm run build",
    "watch:lib": "npm run build:lib -- --watch",
    "test": "ng test ngforage",
    "lint": "alo tslint -p ./projects/tsconfig.lint.json",
    "lint:fix": "npm --scripts-prepend-node-path=auto run lint -- --fix",
    "precompodoc": "rimraf dist/demo/docs",
    "compodoc": "compodoc -c ./.compodocrc.yml"
  },
  "keywords": [
    "localforage",
    "binding",
    "angular",
    "indexeddb",
    "indexed",
    "db",
    "websql",
    "web",
    "sql",
    "local",
    "session",
    "storage",
    "localstorage",
    "sessionstorage",
    "pwa",
    "offline",
    "progressive",
    "web",
    "app"
  ],
  "peerDependencies": {
    "localforage": "^1.5.0"
  },
  "devDependencies": {
    "@alorel-personal/build-tools": "4.7.19",
    "@alorel-personal/conventional-changelog-alorel": "2.1.3",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "5.0.7",
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "^11.1.4",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/language-service": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@compodoc/compodoc": "^1.1.11",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.3",
    "bluebird": "^3.7.2",
    "bootstrap-material-design": "^4.1.3",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.0.2",
